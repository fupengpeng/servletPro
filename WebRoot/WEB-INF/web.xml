<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="WebApp_ID" version="3.1">
	<display-name>servletPro</display-name>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>HelloServlet</servlet-name>
		<servlet-class>com.fupengpeng.servlet.HelloServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>HelloServlet</servlet-name>
		<url-pattern>/HelloServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>MyServlet1</servlet-name>
		<servlet-class>com.fupengpeng.servlet.MyServlet1</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>MyServlet1</servlet-name>
		<url-pattern>/MyServlet1</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>MyInitServlet1</servlet-name>
		<servlet-class>com.fupengpeng.servlet.MyInitServlet1</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>MyInitServlet1</servlet-name>
		<url-pattern>/MyInitServlet1</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>MyInitServlet2</servlet-name>
		<servlet-class>com.fupengpeng.servlet.MyInitServlet2</servlet-class>
		<load-on-startup>2</load-on-startup>
	</servlet>
	<servlet-mapping>
		<servlet-name>MyInitServlet2</servlet-name>
		<url-pattern>/MyInitServlet2</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ServletConfigTest</servlet-name>
		<servlet-class>com.fupengpeng.servlet.ServletConfigTest</servlet-class>
		<init-param>
			<param-name>encoding</param-name>
			<param-value>utf-8</param-value>
		</init-param>
		<init-param>
			<param-name>version</param-name>
			<param-value>1.0</param-value>
		</init-param>
		<init-param>
			<param-name>zuozhe</param-name>
			<param-value>fupengpeng</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>ServletConfigTest</servlet-name>
		<url-pattern>/ServletConfigTest</url-pattern>
	</servlet-mapping>
</web-app>